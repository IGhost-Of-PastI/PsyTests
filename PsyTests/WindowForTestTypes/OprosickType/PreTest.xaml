<Page x:Class="PsyTests.PreTest"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PsyTests"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PreTest">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Image Grid.Row="0"></Image>
        <TextBlock x:Name="Name" Text="" Grid.Row="1"></TextBlock>
        <TextBlock x:Name="Opis" Text="" Grid.Row="2"></TextBlock>
        <TextBlock x:Name="Algorith" Text="" Grid.Row="3"></TextBlock>
        <Button x:Name="Start" Content="Начать" Grid.Row="4" Click="Start_Click"></Button>
    </Grid>
</Page>
