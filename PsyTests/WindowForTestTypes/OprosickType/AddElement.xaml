<Window x:Class="PsyTests.WindowForTestTypes.OprosickType.AddElement"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PsyTests.WindowForTestTypes.OprosickType"
        mc:Ignorable="d"
        Title="AddElement" Height="100" Width="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Label Content="Название:"></Label>
            <TextBox x:Name="StringName"></TextBox>
        </StackPanel>
        <StackPanel Grid.Row="1" Orientation="Horizontal">
            <Button Content="Ок" Click="Button_Click"></Button>
            <Button Content="Отмена" Click="Button_Click_1"></Button>
        </StackPanel>
    </Grid>
</Window>
